<h1 class='text-center'>All users</h1>

<div class="row">
  <div class="col-md-8 col-md-offset-2"><%= will_paginate %> </div>
</div>

  <ul class="users">
    <% @user.each do |user| %>
      <li>
        <%= link_to user.name, user %>
        <%= link_to "Delete User", user, method: :delete, data: { confirm: "Delete?" }, class: 'btn btn-danger', id: 'DeleteUser'%>
      </li>
      <% end %>
    </ul>

    

<div class="row">
  <div class="col-md-8 col-md-offset-2"><%= will_paginate %> </div>
</div>
